{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch index.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}//src//index.js",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}//src//",
            "runtimeArgs": [
                "--max-old-space-size=16384"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch convertFromOSM.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}//src//convertFromOSM.js",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}//src//",
            "args": ["--noCacheWrite"],
            "runtimeArgs": [
                "--max-old-space-size=16384",
                "--expose-gc"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch MapAreasToImage.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}//src//MapAreasToImage.js",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}//src//"
        }
    ]
}